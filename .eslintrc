{
  "env": {
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-recommended",
    "prettier"
  ],
  "parser": "vue-eslint-parser",
  "rules": {
    "no-console": "warn",
    "comma-dangle": "warn",
    "no-unused-vars": "error",
    "no-var": "error",

    "vue/no-unused-vars": "error"
  }
}
